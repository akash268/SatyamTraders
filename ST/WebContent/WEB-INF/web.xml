<?xml version="1.0" encoding="ISO-8859-1"?>

<!DOCTYPE web-app 
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN" 
    "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>

    <display-name>
      Satyam Traders 
    </display-name>
    <description>
      Satyam Traders, Patanjli Product Dealer
    </description>

    

    <servlet>
      <servlet-name>itemServlet</servlet-name>
      <description></description>
      <servlet-class>satyamtraders.servlets.ItemsServlet</servlet-class>
      
    </servlet>

    <servlet-mapping>
      <servlet-name>itemServlet</servlet-name>
      <url-pattern>/Items</url-pattern>
    </servlet-mapping>


    <session-config>
      <session-timeout>30</session-timeout> 
    </session-config>

</web-app>